{
  "builds": [
    {
      "src": "public/bundle.js",
      "use": "@vercel/static"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/bundle.js",
      "dest": "/public/bundle.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
