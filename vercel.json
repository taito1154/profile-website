{
  "builds": [
    {
      "src": "src/main.js",
      "use": "@vercel/static"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "public": "public",
  "routes": [
    {
      "src": "/main.js",
      "dest": "/src/main.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
